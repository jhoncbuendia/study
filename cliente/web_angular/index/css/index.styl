.bg-search
  padding: 10px 15px;
  background: #fff;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow: 0 1px 2px rgba(0,0,0,0.25);
  box-shadow: 0 1px 2px rgba(0,0,0,0.25);
  margin-top: 20px
  margin-bottom: 20px

#top-bar
