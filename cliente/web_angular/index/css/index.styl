body
  background: #f5f5f5

input
  border-radius: 0px !important

.row
  margin-right: 0 !important;

.video-container
  width: 100%;
  position: relative;


.video-background
  width: 100%

.card
  width: 90%
  margin: 0 auto
  border: 1px solid #e5e5e5;
  border-radius: .25rem;
  padding: 10px
